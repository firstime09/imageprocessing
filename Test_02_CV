import cv2, os
import cv2, os
import numpy as np
import matplotlib.pyplot as plt
from google.colab import drive

img_ori = cv2.imread("Bakteri_A.jpeg")
img_gray = cv2.imread("Bakteri_A.jpeg",0)

"""Plotting image"""
f = plt.figure()
f.add_subplot(1,2, 1)
plt.title("Image ori")
plt.imshow(img_ori)
f.add_subplot(1,2, 2)
plt.title("Image gray")
plt.imshow(img_gray)
